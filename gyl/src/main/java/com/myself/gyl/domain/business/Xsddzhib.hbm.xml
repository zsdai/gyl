<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.myself.gyl.domain.business.Xsddzhib">
		<id name="xsddzhibid" length="5">
			<generator class="increment"></generator>
		</id>
		<!-- 商品编码 -->
		<property name="spbm" length="100"></property>
		<!-- 商品名称 -->
		<property name="spmc" length="100"></property>
		<!-- 单品扣率 -->
		<property name="dpkl"></property>
		<!-- 
			来源行号
		 -->
		<property name="lyhh"></property>
		<!-- 发货仓库 -->
		<property name="fhck" length="20"></property>
		<!-- 行号 -->
		<property name="hh" length="5"></property>
		<!-- 含税单价 -->
		<property name="hsdj"></property>
		<!-- 含税金额 -->
		<property name="hsje"></property>
		<!-- 行状态 -->
		<property name="hstatus"></property>
		<!-- 是否出库关闭 -->
		<property name="isckgb"></property>
		<!-- 是否发货关闭 -->
		<property name="isfhgb"></property>
		<!-- 是否开票关闭 -->
		<property name="iskpgb"></property>
		<!-- 是否收款结束 -->
		<property name="isskjs"></property>
		<!-- 计划发货日期 -->
		<property name="jhfhrq"></property>
		<!-- 计量单位 -->
		<property name="jldw"></property>
		<!-- 累计出库数量 -->
		<property name="ljcksl"></property>
		<!-- 累计发货数量 -->
		<property name="ljfhsl"></property>
		<!-- 累计开票数量 -->
		<property name="ljkpsl"></property>
		<!-- 累计应收数量 -->
		<property name="ljyssl"></property>
		<!-- 税额 -->
		<property name="se"></property>
		<!-- 收货单位 -->
		<property name="shdw"></property>
		<!-- 税率 -->
		<property name="shulv"></property>
		<!-- 数量 -->
		<property name="sl"></property>
		<!-- 无税单价 -->
		<property name="wsdj"></property>
		<!-- 无税金额 -->
		<property name="wsje"></property>
		<!-- 型号 -->
		<property name="xh"></property>
		<!-- 应发未出库数量 -->
		<property name="yfwcksl" length="5"></property>
		<!-- 要求收货日期 -->
		<property name="yqshrq"></property>
		<!-- 折扣额 -->
		<property name="zke"></property>
		<!-- 赠品 -->
		<property name="zp"></property>
		<many-to-one name="xsddzhub" class="com.myself.gyl.domain.business.Xsddzhub"
			column="xsddzhubid"></many-to-one>
	</class>
</hibernate-mapping>
