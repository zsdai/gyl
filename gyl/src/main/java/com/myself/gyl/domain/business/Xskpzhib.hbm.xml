<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.myself.gyl.domain.business.Xskpzhib">
		<id name="xskpzhibid" length="5">
			<generator class="increment"></generator>
		</id>
		
		<!-- 
			累计开票数量
		 -->
		<property name="ljkpsl" length="5"></property>
		<!-- 
			仓库
		 -->
		<property name="ck" length="20"></property>
		<!-- 
			单品折扣
		 -->
		<property name="dpkl"></property>
		<!-- 
			单位
		 -->
		<property name="dw" length="20"></property>
		<!-- 
			行号
		 -->
		<property name="hh"></property>
		<!-- 
			含税单价
		 -->
		<property name="hsdj"></property>
		<!-- 
			含税金额
		 -->
		<property name="hsje"></property>
		<!-- 
			是否开票结束
		 -->
		<property name="iskpjs"></property>
		
		<!-- 
			来源单据号
		 -->
		<property name="lydjh" length="50"></property>
		<!-- 
			来源行号
		 -->
		<property name="lyhh" length="5"></property>
		<!-- 
			收货单位
		 -->
		<property name="shdw" length="100"></property>
		<!-- 
			税率
		 -->
		<property name="shulv"></property>
		<!-- 
			数量
		 -->
		<property name="sl" length="5"></property>
		<!-- 
			商品编码
		 -->
		<property name="spbm" length="100"></property>
		<!-- 
			商品名称
		 -->
		<property name="spmc" length="100"></property>
		
		<!-- 
			无税单价
		 -->
		<property name="wsdj"></property>
		<!-- 
			无税金额
		 -->
		<property name="wsje"></property>
		<!-- 
			型号
		 -->
		<property name="xh" length="50"></property>
		<!-- 
			源头单据号
		 -->
		<property name="ytdjh" length="100"></property>
		<!-- 
			源头行号
		 -->
		<property name="ythh" length="5"></property>
		<!-- 
			折扣额
		 -->
		<property name="zke"></property>
		<!-- 
			赠品
		 -->
		<property name="zp"></property>
		<many-to-one name="xskpzhub" class="com.myself.gyl.domain.business.Xskpzhub" column="xskpzhubid"></many-to-one>
	</class>
</hibernate-mapping>
